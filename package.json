{
  "name": "synergy-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.6.2",
    "axios": "^0.26.1",
    "connected-react-router": "^6.9.2",
    "history": "4.10.1",
    "immer": "^9.0.12",
    "prop-types": "^15.8.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "7.2.5",
    "react-remarkable": "^1.1.3",
    "react-router": "^5.2.1",
    "react-scripts": "^5.0.0",
    "redux": "4.1.1",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --ci --coverage --reporters=jest-junit --reporters=default --coverageReporters=cobertura --coverageReporters=html"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "axios-mock-adapter": "^1.17.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "intl": "^1.2.5",
    "jest-extended": "^0.11.2",
    "jest-junit": "^8.0.0",
    "jest-junit-reporter": "^1.1.0",
    "jest-watch-typeahead": "^0.6.5",
    "jest-watcher": "^27.5.1",
    "msw": "^0.35.0",
    "node-sass": "^7.0.1",
    "react-markdown": "^8.0.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
